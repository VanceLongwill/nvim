{
  "coc.preferences.useQuickfixForLocations": true,
  // "list.source.files.command": "rg --files --vimgrep .",
  "yaml.format.enable": true,
  "tsserver.enable": true,
  "tsserver.log": "verbose",
  "tsserver.npm": "/Users/vance/.nvm/versions/node/v14.19.3/bin/node",
  "typescript.suggest.enabled": true,
  "typescript.validate.enable": true,
  "typescript.suggest.paths": true,
  "eslint.nodePath": "./node_modules/eslint",
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "typescript.tsx",
    "javascript.jsx"
  ],
  "typescript.referencesCodeLens.enable": true,
  "git.enableGutters": true,
  "list.normalMappings": {
    "o": "action:open",
    "t": "action:tabe",
    "v": "action:vsplit",
    "s": "action:split",
    "d": "expr:GetDeleteAction"
  },
  "list.insertMappings": {
    "<C-o>": "action:open",
    "<C-t>": "action:tabe",
    "<C-w>": "command:wincmd k"
  },
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod",
        ".git/"
      ],
      "filetypes": [
        "go"
      ]
    },
    "golangci-lint-languageserver": {
      "command": "golangci-lint-langserver",
      "filetypes": [
        "go"
      ],
      "initializationOptions": {
        "command": [
          "golangci-lint",
          "run",
          "--out-format",
          "json"
        ]
      }
    },
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    }
  }
}
